%h1
  setup branches
  %a{class: :add, href: new_admin_setup_path } +
%hr
- @branches.each do |branch|
  .branch
    %a{href: edit_admin_setup_path(branch) } Setup #{branch.name}
    =link_to 'x', admin_setup_path(branch), :method => :delete, :data => { :confirm => "Delete #{branch.name}\nAre you sure?" }